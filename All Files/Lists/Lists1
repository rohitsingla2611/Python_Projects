list1 = [1, 'Rohit', 'Singla', 'BCA', 26, '-', 11, '-', 1997]
list2 = [2, 'Suresh', 'Jain', 10, '-', 0o7, '-', 1972]

print(list1)
print(list2)

list1.append('Student')
print(list1)

list2.extend(['Buissness', 'Man'])
print(list2)

list2.insert(4, 'Hello')
print(list2)

list2.pop()
print(list2)

list2.pop()
print(list2)

list2.pop()
print(list2)
